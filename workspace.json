    "demoshop": {
      "root": "apps/demoshop",
      "sourceRoot": "apps/demoshop",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/next:build",
          "options": {
            "root": "apps/demoshop",
            "outputPath": "dist/apps/demoshop"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "builder": "@nrwl/next:server",
          "options": {
            "buildTarget": "demoshop:build",
            "dev": true,
            "proxyConfig": "apps/demoshop/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "demoshop:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "builder": "@nrwl/next:export",
          "options": {
            "buildTarget": "demoshop:build:production"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/demoshop/.eslintrc",
            "tsConfig": [
              "apps/demoshop/tsconfig.json",
              "apps/demoshop/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/demoshop/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/demoshop/jest.config.js",
            "tsConfig": "apps/demoshop/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
